{
    "compilerOptions": {
        "target": "es6",
        "module": "NodeNext",
        "rootDir": ".",
        "baseUrl": ".",
        "moduleResolution": "NodeNext",
        "skipLibCheck": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "strict": true,
        "strictNullChecks": false,
        "strictPropertyInitialization": false
    }
}
