{
  "name": "@mortgagecalc/siteservice",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^8.1.1",
    "express": "^4",
    "@sap/xssec": "^4.1.3",
    "hdb": "0.19.8",
    "@sap-cloud-sdk/http-client": "3.18.1",
    "@mortgagecalc/foundation": "*",
    "@mortgagecalc/calculatorservice": "*",
    "@mortgagecalc/database": "*"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.25.0",
    "@cap-js/cds-types": "^0.6.3",
    "@cap-js/sqlite": "^1",
    "@sap/eslint-plugin-cds": "^3",
    "eslint": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "imports": {
    "#cds-models/db/*": "@mortgagecalc/database/*",
    "#cds-models/*": {
      "require": {
        "default": "./@cds-models/*/index.js",
        "types": "./@cds-models/*/index.ts"
      },
      "import": {
        "default": "./@cds-models/*/index.js",
        "types": "./@cds-models/*/index.ts"
      }
    }
  },
  "cds": {
    "requires": {
      "CalculatorService": {
        "kind": "odata",
        "model": "srv/external/CalculatorService"
      }
    }
  }
