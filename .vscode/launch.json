{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds-ts serve",
      "command": "npx cds-ts serve",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "cds-ts serve production",
      "command": "npx cds-ts serve --production",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "cds serve",
      "command": "npx cds serve",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "attach to node process",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "localRoot": "${workspaceFolder}/srv/calculator",
      "preLaunchTask": "tsc: build - srv/calculator/tsconfig.json",
      "outFiles": ["${workspaceFolder}/srv/calculator/gen/srv/**/*.js"]
    }
  ]
}
